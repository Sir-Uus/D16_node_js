<main role="main" class="container">
    <h1 class="mt-5">My Website</h1>
    <h2>List Data karyawan Pak Budi</h2>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Tanggal Lahir</th>
                <th scope="col">Gender</th>
                <th scope="col">Alamat</th>
                <th scope="col">Kontak</th>
            </tr>
        </thead>
        <tbody>
            <% karyawanBudi.forEach(karyawan=>{ %>
                <tr>
                    <td>
                        <%= karyawan.name %>
                    </td>
                    <td>
                        <%= karyawan.tanggal %>
                    </td>
                    <td>
                        <%= karyawan.kelamin %>
                    </td>
                    <td>
                        <%= karyawan.alamat %>
                    </td>
                    <td>
                        <%= karyawan.kontak %>
                    </td>
                </tr>
            <% }); %>
        </tbody>
        
    </table>
</main>